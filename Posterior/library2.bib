@phdthesis{DeInvestigacion2020,
abstract = {Spectroscopy is the study of the interaction between electromagnetic radiation and matter, usually using spectrometers: devices with the capacity of analyzing the frequency spectrum of an electromagnetic radiation source. Nevertheless, there is an important challenge: to eliminate the instrument response function. This paper focuses on measurements of solar spectral irradiance in its direct and diffuse components and the sum of both, which is called the global component. For this, a mini-spectrometer is used, which required calibration and the use of some techniques to minimize the effect of the instrument response function. The measurement system consists of a cosine corrector, whose diffuser material is spectralon, secured to an Ocean Optics QP-400-2-UV-VIS optical fiber and a spectrometer with a spectral response of 200 to 1100 nanometers of wavelength. The system is connected to a PC using the Spectra Suite software from Ocean Optics. To obtain the direct component of solar radiation, a cylindrical aluminum part was created with a total aperture of 7.24° that eliminates the diffuse component. To obtain the diffuse component, another piece was created, also with an aperture of 7.24°, which eliminates the beam of direct solar radiation. To measure the global component, only the cosine corrector coupled to the spectrometer was used, the former has a field of view of 2$\pi$ sr. The reports of measured solar spectral irradiance are scarce in the literature, so the development of the present project derives in multiple benefits in the field of variable spectrum solar simulators, characterization of photovoltaic cells, photochemistry, photobiology, as well as applications in dermatology. The results obtained using the proposed system were validated with those obtained by means of a calibrated pyranometer. Finally, a methodology for obtaining spectral solar irradiances and the creation with them of a database is proposed.},
author = {Garcia, Alejandro. and Nacional., Instituto Polit{\'{e}}cnico},
file = {:D$\backslash$:/Iv{\'{a}}n/Maestr{\'{i}}a/Documentos/OneDrive - Instituto Politecnico Nacional/master/Documentos/DISE{\~{N}}O DE UN SISTEMA PORT{\'{A}}TIL DE MEDICI{\'{O}}N.pdf:pdf},
pages = {99},
school = {Instituto Politecnico Nacioal},
title = {{Instituto Polit{\'{e}}cnico Nacional}},
year = {2020}
}
