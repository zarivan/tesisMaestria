file = {:D$\backslash$:/Iv{\'{a}}n/Maestr{\'{i}}a/Documentos/OneDrive - Instituto Politecnico Nacional/master/Documentos/ID107.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
journal = {Journal of Chemical Information and Modeling},
keywords = {icle},
