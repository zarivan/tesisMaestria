@article{Salgado2009,
abstract = {Un filtro es un circuito electr{\'{o}}nico que posee una entrada y una salida. En la entrada se introducen se{\~{n}}ales alternas de diferentes frecuencias y en la salida se extraen esas se{\~{n}}ales atenuadas en mayor o menor medida seg{\'{u}}n la frecuencia de la se{\~{n}}al. Si el circuito del filtro est{\'{a}} formado por resistencias, condensadores y/o bobinas (componentes pasivos) el filtro se dir{\'{a}} que es un filtro pasivo. Si el circuito del filtro est{\'{a}} formado por el esquema o c{\'{e}}lula b{\'{a}}sica se dir{\'{a}} que es de primer orden. Ser{\'{a}} de segundo orden si est{\'{a}} formado por dos c{\'{e}}lulas b{\'{a}}sicas, de tercer orden si lo esta por tres, etc. Frecuencia de corte: Es la frecuencia para la que la ganancia en tensi{\'{o}}n del filtro cae de 1 a 0.707 (esto expresado en decibelios, dB, se dir{\'{i}}a como que la ganancia del filtro se reduce en 3dB de la m{\'{a}}xima, que se considera como nivel de 0dB). En los filtros pasa banda y elimina banda existir{\'{a}}n dos frecuencias de corte diferentes, la inferior y la superior. Banda de Paso: Es el margen de frecuencias para las cuales la se{\~{n}}al se aten{\'{u}}a {\textless}3dB, y est{\'{a}} comprendida dentro de las frecuencias de corte Filtros pasa bajos, pasa altos, pasa banda y elimina banda:},
author = {Salgado, Javier},
file = {:D$\backslash$:/Iv{\'{a}}n/Maestr{\'{i}}a/Documentos/OneDrive - Instituto Politecnico Nacional/master/Documentos/Unknown/Salgado/Salgado - 2009 - Los Filtros Pasivos de Primer Orden.pdf:pdf},
pages = {8,9},
title = {{Los Filtros Pasivos de Primer Orden}},
url = {http://ele-mariamoliner.dyndns.org/{~}jsalgado/analogica/6CA-filtros.pdf},
year = {2009}
}
