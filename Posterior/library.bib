file = {:D$\backslash$:/Iv{\'{a}}n/Maestr{\'{i}}a/Documentos/OneDrive - Instituto Politecnico Nacional/master/Documentos/Sensors and Actuators A Physical/Mishra et al/Mishra et al. - 2011 - Fiber grating sensors in medicine Current and emerging applications.pdf:pdf},
issn = {0924-4247},
